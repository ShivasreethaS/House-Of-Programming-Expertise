import java.util.*;
class factFromPast {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int s=sc.nextInt();
        int[] dp=new int[s+1];
        dp[0]=1;
        dp[1]=1;
        for(int i=2;i<=s;i++){
            dp[i]=dp[i-1]*i;
        }
        for(int i=0;i<=s;i++){
            System.out.println(dp[i]);
        }
        System.out.print(Arrays.toString(dp)); //prints as a list
    }
}
